<script>
        $('.filtercheckbox').change(function(){
            var isOpen = $('#opennow').is(':checked'),
                goodRating = $('#fourstars').is(':checked'),
                verified = $('#verified').is(':checked'),
                hasMenu = $('#hasmenu').is(':checked');
            
            $('.single-shop').each(function(){
                var hide = false;
                if(isOpen){
                    if(!$(this).data('isopen')){
                        hide = true;
                    }
                };
                
                if(goodRating){
                    if(!$(this).data('goodrating')){
                        hide = true;
                    }
                };
                
                if(verified){
                    if(!$(this).data('verified')){
                        hide = true;
                    }
                };
                
                if(hasMenu){
                    if(!$(this).data('hasmenu')){
                        hide = true;
                    }
                };
                if(hide){
                    $(this).hide();
                }else{
                    $(this).show();
                }
            });
            if($('.single-shop:visible').length < 1 ){
                $('.coffeeshopsoverzicht').addClass('empty');
            }else{
                $('.coffeeshopsoverzicht').removeClass('empty');
            }
        });
    </script>
<style>
        .coffeeshopsoverzicht.empty {
            position: relative;
            min-height: 100px;
        }
        .coffeeshopsoverzicht.empty::after {
            content: 'Geen resultaten';
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 16px;
            color: #000;
            opacity: 0.5;
            font-weight: 400;
        }
    </style>
