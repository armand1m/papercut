<script>
    $('#loginform').submit(function(e){
        e.preventDefault();
        $('#loginnowbtn').attr('disabled', true).addClass('disabled');
        setTimeout(function(){
            $.ajax({
                url: 'json/login',
                type: "POST",
                async: false,
                data: {
                    user: $('#loginuser').val(),
                    pass: $('#loginpass').val()
                },
                success: function (result) {
                    if (result.success) {
                        Cookies.set('cookieuser', result.cookieuser, { expires: 30, domain: '.greenmeister.nl' });
                        Cookies.set('secret', result.secret, { expires: 30, domain: '.greenmeister.nl' });
                        Cookies.set('token', result.token, { expires: 30, domain: '.greenmeister.nl' });
                        Cookies.set('si', result.si, { expires: 30, domain: '.greenmeister.nl' });
                        location.reload();
                    } else {
                        $('#loginerror').html(result.message);
                    }
                    $('#loginnowbtn').attr('disabled', false).removeClass('disabled');
                },
                error: function () {
                    $('#loginerror').html('Er is een onbekende fout opgetreden...');
                    $('#loginnowbtn').attr('disabled', false).removeClass('disabled');
                }
            });
        }, 100);
    });

    $('#loginform').keyup(function (e) {
        var code = e.which;
        if (code != 13) {
            $('#loginerror').html('');
        }
    });

    $('#registerform').submit(function (e) {
        e.preventDefault();
        $('#registernowbtn').addClass('disabled').attr('disabled', true);
        setTimeout(function(){
            $.ajax({
                url: 'json/register',
                type: "POST",
                async: false,
                data: {
                    user: $('#registeruser').val(),
                    pass: $('#registerpass').val(),
                    email: $('#registeremail').val()
                },
                success: function(result) {
                    if (result.success) {
                        $('.loginregistermodal .tab').remove();
                        $('.loginregistermodal .inner').html("<div style='text-align:center;width:100%;width:calc(100% - 40px);padding:20px;'><h1 id='plsconfirmemail'>Bevestig je emailadres!</h1><p id='plsconfirmemailtext'>Er is een bevestigingslink naar je emailadres verzonden. Klik op de link om je account te activeren.</p></div>");
                    } else {
                        $('#registererror').text(result.message);
                    }
                    $('#registernowbtn').removeClass('disabled').attr('disabled', false);
                },
                error: function () {
                    $('#registererror').html('Er is een onbekende fout opgetreden. Heb je wel een internetverbinding?');
                    $('#registernowbtn').removeClass('disabled').attr('disabled', false);
                }
            });
        },100);
    });

    $('#registerform').keyup(function (e) {
        var code = e.which;
        if (code != 13) {
            $('#registererror').html('');
        }
    });
    
    $('body').on('click', '#verifyemailclose', function(){
        $('body').find('#verifyemailwrap').fadeOut();    
    });
    
    $('.switchloginmodal').click(function(){
        $('.loginregistermodal').toggleClass('forgotpw');
    });
    
    $('#forgotpwform').submit(function(e){
        e.preventDefault();
        $('#resetpwbtn').attr('disabled', true).addClass('disabled');
        setTimeout(function(){
            var reqEmail = $('#forgotpwemail').val();
            $.ajax({
                url         : 'json/profilefunctions',
                type        : "POST",
                async       : false,
                data        : {
                    forgotpw    : reqEmail
                        },
                success : function(result) {
                    if(result.success){
                        $('#forgotpwinnerwrap').html("<p>Indien het ingevoerde emailadres bij een account hoort is er een link verzonden, waarmee het wachtwoord opnieuw kan worden ingesteld.</p>");
                    }else{
                        alert(result.message);
                    }
                    $('#resetpwbtn').attr('disabled', false).removeClass('disabled');
                },
                error        : function() {
                    alert('Er is een onbekende fout opgetreden. Heb je wel een internetverbinding?');
                    $('#resetpwbtn').attr('disabled', false).removeClass('disabled');
                }
            });
        }, 100);
    });
    
</script>
<style>
    #register {
        display: block;
    }

