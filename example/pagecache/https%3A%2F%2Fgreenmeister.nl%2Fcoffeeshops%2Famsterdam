<script>
      $(document).ready(function(){
        /* NodumScriptStart */
$(window).on("scroll", function(e) {
    if (window.pageYOffset > 136) {
        $('.sidebar').addClass("posfixed");
    } else {
        $('.sidebar').removeClass("posfixed");
    }
});


var xhr;
new autoComplete({
    selector: 'input[name="shopsearch"]',
    source: function(term, response){
        try { xhr.abort(); } catch(e){}
        xhr = $.getJSON('/json/autocomplete', { search: term }, function(data){ response(data); });
    }
});


if (!navigator.geolocation){
    $('.uselocation').hide();
    $('.of').hide();
}

$('.uselocation').click(function(){
    if (!navigator.geolocation){
        alert('Your browser doesn\'t support this functionality.');
        return;
    }
    
    function success(position) {
        var latitude  = position.coords.latitude;
        var longitude = position.coords.longitude;
        window.location.href = "/coffeeshops?lat="+latitude+"&lng="+longitude;
    }
    
    function error() {
        $('.uselocation').html('Error.. Try again?');
    }
    if($(window).width() < 993 ){
        $('#shopsearch').val($findinglocationtext).attr('disabled', true);
        $('#newsearch').addClass('disabled');
    }else{
        $('.uselocation').html($findinglocationtext);
    }
    navigator.geolocation.getCurrentPosition(success, error, {'timeout':10000});
    function error(err) {
        $('#shopsearch').val('').attr('disabled', false);
        $('#newsearch').removeClass('disabled');
    }
});
        /* NodumScriptEnd */
       });
    </script>
