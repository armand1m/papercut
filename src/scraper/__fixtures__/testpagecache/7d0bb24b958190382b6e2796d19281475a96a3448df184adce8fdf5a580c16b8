    <script>
    function fetchListingPhone( element ){
        element.unbind("click");

        $.post( "/item/phone", { item: element.data("item"), type: element.data("type")})
            .done( function (response) {
                if( response.status ) {
                    element.html(response.data);
                    element.removeClass();
                } else {
                    }
            }
        );
    }

    $('.listingContactButton').click( function () {
        fetchListingPhone($(this));
    });

    $('#sendMail').on('hidden.bs.modal', function(e) {
        $(this).removeData('bs.modal');
    });
</script>
<script>
    $(document).ready(function () {
        eDirectorySummaryMap = new eDirectory.Search.Map(
            "/search/map/summary",
            summaryMap,
            summaryMap_container,
            summaryMarkerClusterer,
            '#summaryViewMapButton',
            '#mapMarkerSummaryBox',
            30
        );

        eDirectorySummaryMap.initialize();
    });
